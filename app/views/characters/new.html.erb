<h1>New Character</h1>

<%= form_tag '/characters' method: :post do  %>
	<div>
		<%= label_tag :first_name %>
		<%= text_field_tag :first_name %>
	</div>	
	<div><%= label_tag :house %>
		<%= text_field_tag :house %>
			
	</div>
	<div><%= label_tag :city %>
		<%= text_field_tag :city %>
			
	</div>
	<div><%= submit_tag "Create Character" %></div>
		
		


	
<% end %>